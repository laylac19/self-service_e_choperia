	<parameter name="DataInicial" class="java.sql.Date"/>
	<parameter name="DataFinal" class="java.sql.Date"/>
	<queryString>
		<![CDATA[SELECT p.id,
	COUNT(*) AS total_consumido,
	p.descricao,
	p.qtde_estoque,
	p.preco_compra,
	p.preco_venda
FROM produto p
	JOIN cliente_compra_produto ccp ON 
	 p.id = ccp.produto_id 
WHERE ccp.data_compra BETWEEN $P{DataInicial} AND $P{DataFinal} 
GROUP BY p.id,
	p.descricao
ORDER BY total_consumido DESC]]>
	</queryString>
